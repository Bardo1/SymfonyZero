{% extends '::base.html.twig' %}
{% block stylesheets %}
    {{parent()}}
    {% stylesheets 
    'bundles/BootstrapBundle/Resources/public/css/selvira.customview.css'
    'bundles/BootstrapBundle/Resources/public/css/bootstrap.css'
    'bundles/BootstrapBundle/Resources/public/css/bootstrap-responsive.css'
    filter='cssrewrite' debug=false  output='css/mycss.css'%}
    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />        
    {% endstylesheets %}
{% endblock %}

{% block body -%}
{% endblock %}

{% block javascripts %}
    {{parent()}}
    {% javascripts 
    'bundles/BootstrapBundle/Resources/public/js/jquery-3.1.1.min.js'
    'bundles/BootstrapBundle/Resources/public/js/bootstrap.min.js'
    output="js/myscript.js" debug=false %}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}