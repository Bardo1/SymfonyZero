{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{parent()}}
    {% stylesheets
    'bundles/bootstrap/css/selvira.customview.css'
    'bundles/bootstrap/css/bootstrap.css'
    'bundles/bootstrap/css/bootstrap-responsive.css'
    filter='cssrewrite' debug=false  output='css/app.css'%}
    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />        
    {% endstylesheets %}
{% endblock %}

{% block body -%}
{% endblock %}

{% block javascripts %}
    {{parent()}}
    {% javascripts 
    'bundles/bootstrap/js/jquery-3.1.1.min.js'
    'bundles/bootstrap/Resources/public/js/bootstrap.min.js'
    output="js/myscript.js" debug=false %}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}